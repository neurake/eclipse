		int t=sc.nextInt();
		System.out.print("Enter N:");
		int n=sc.nextInt();
		int arr[]=new int[n];
		System.out.println("Enter k:");
		int k=sc.nextInt();
		System.out.println("Enter array:");
		for(int i=0;i<n;i++)
		{
			arr[i]=sc.nextInt();
		}
		
		System.out.println("Your  array is:");
		for(int i=0;i<n;i++)
		{
			System.out.println(arr[i]);
		}
		

	}

}
